{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "outDir": "./Xslab",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "Xslab"]
}
